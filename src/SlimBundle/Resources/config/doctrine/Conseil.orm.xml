<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SlimBundle\Entity\Conseil" table="conseil">
    <id name="idConseil" type="integer" column="id_conseil">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="string" column="type" length="50" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="contenu" type="string" column="contenu" length="10000" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
