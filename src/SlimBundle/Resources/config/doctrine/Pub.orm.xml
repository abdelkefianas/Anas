<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="SlimBundle\Entity\Pub" table="pub">
    <id name="idpublicite" type="integer" column="idpublicite">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nompublicite" type="string" column="nompublicite" length="100" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="datepub" type="date" column="datepub" nullable="false"/>
    <field name="nomproduit" type="string" column="nomproduit" length="40" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
